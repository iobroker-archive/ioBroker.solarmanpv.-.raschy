{
	"i18n": true,
	"type": "tabs",
	"items": {
		"_credentials": {
			"type": "panel",
			"label": "credentials",
			"items": {
				"email": {
					"newLine": true,
					"type": "text",
					"xs": 12,
					"sm": 12,
					"md": 6,
					"lg": 4,
					"xl": 4,
					"label": "email"
				},
				"password": {
					"type": "password",
					"repeat": false,
					"visible": false,
					"xs": 12,
					"sm": 12,
					"md": 6,
					"lg": 4,
					"xl": 4,
					"label": "password"
				},
				"appId": {
					"type": "number",
					"xs": 12,
					"sm": 12,
					"md": 6,
					"lg": 4,
					"xl": 4,
					"label": "appId",
					"help": "appIdfromSolarmanpv"
				},
				"appSecret": {
					"type": "text",
					"xs": 12,
					"sm": 12,
					"md": 6,
					"lg": 4,
					"xl": 4,
					"label": "appSecrets",
					"help": "secretsFromSolarmanpv"
				},
				"companyName": {
					"type": "text",
					"xs": 12,
					"sm": 12,
					"md": 6,
					"lg": 4,
					"xl": 4,
					"label": "companyName",
					"help": "nameOfCompany"
				}
			}
		},
		"_blacklist": {
			"type": "panel",
			"label": "valuesSelection",
			"items": {
				"_headerBlacklist": {
					"newLine": true,
					"type": "header",
					"text": "excludedValues",
					"size": 2
				},
				"deviceBlacklist": {
					"newLine": true,
					"type": "chips",
					"delimiter": ",",
					"label": "termsToBeExcluded",
					"help": "designateTheValues",
					"xs": 12,
					"sm": 12,
					"md": 6,
					"lg": 4,
					"xl": 4
				},
				"headerZero": {
					"newLine": true,
					"type": "header",
					"text": "zeroedValues",
					"size": 2
				},
				"deviceZero": {
					"newLine": true,
					"type": "chips",
					"delimiter": ",",
					"xs": 12,
					"sm": 12,
					"md": 6,
					"lg": 4,
					"xl": 4,
					"label": "termsToBeZeroed"
				}
			}
		},
		"_systemModules": {
			"type": "panel",
			"label": "systemModules",
			"xs": 12,
			"sm": 12,
			"md": 6,
			"lg": 4,
			"xl": 4,
			"items": {
				"_headerModule": {
					"newLine": true,
					"type": "header",
					"text": "selectedSubModules",
					"size": 2
				},
				"deviceModules": {
					"type": "table",
					"newLine": true,
					"noDelete": true,
					"xs": 12,
					"sm": 12,
					"md": 12,
					"lg": 12,
					"xl": 12,
					"items": [
						{
							"type": "text",
							"noClearButton": true,
							"title": "modul",
							"attr": "modul",
							"width": "30%"
						},
						{
							"type": "checkbox",
							"title": "selected",
							"attr": "checkSelect",
							"xs": 12,
							"sm": 12,
							"md": 6,
							"lg": 4,
							"xl": 4
						}
					]
				},
				"clearModules": {
					"type": "checkbox",
					"label": "clearSubModules",
					"tooltip": "tooltipClearModuleList",
					"help": "Helptext possible",
					"default": false,
					"newLine": true
				}
			}
		}
	}
}