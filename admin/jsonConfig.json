{
    "i18n": true,
    "type": "tabs",
    "items": {
        "_credentials": {
            "type": "panel",
            "label": "credentials",
            "items": {
                "email": {
                    "newLine": true,
                    "type": "text",
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "label": "email"
                },
                "password": {
                    "type": "password",
                    "repeat": false,
                    "visible": false,
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "label": "password"
                },
				"appId": {
                    "type": "number",
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "label": "appId",
                    "help": "appIdfromSolarmanpv"
                },
                "appSecret": {
                    "type": "text",
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
					"label": "appSecrets",
                    "help": "secretsFromSolarmanpv"
                },
				"companyName": {
                    "type": "text",
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
					"label": "companyName",
                    "help": "nameOfCompany"
                }
            }
        },
		"_blacklist": {
			"type": "panel",
			"label": "valuesSelection",
			"items": {
				"_headerBlacklist": {
					"newLine": true,
					"type": "header",
					"text": "excludedValues",
					"size": 2
				},
				"deviceBlacklist": {
					"newLine": true,
					"type": "chips",
					"delimiter": ",",
					"label": "termsToBeExcluded",
					"help": "designateTheValues",
					"sm": 12
				},  
				"headerZero": {
					"newLine": true,
					"type": "header",
					"text": "zeroedValues",
					"size": 2
				},
				"deviceZero": {
					"newLine": true,
					"type": "chips",
					"delimiter": ",",
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
					"label": "termsToBeZeroed"
   				}
			}
		},
		"_systemModules": {
			"type": "panel",
			"label": "systemModules",
			"items": {
				"_headerModule": {
					"newLine": true,
					"type": "header",
					"text": "selectedSubModules",
					"size": 2
				},
				"deviceModules": {
					"type": "table",
					"newLine": true,
					"sm": 6,
					"md": 6,
					"lg": 6,
					"items": [
						{
							"type": "number",
							"title": "modul",
							"attr": "modul",
							"width": "10%"
						},
						{
							"type": "checkbox",
							"title": "selected",
							"attr": "checkSelect",
							"width": "5%"
						}
					]
				},
				"clearModules": {
                    "type": "checkbox",
                    "label": "clearSubModules",
                    "tooltip": "tooltipClearModuleList",
                    "help":"Helptext possible",
                    "default":false
                }
			}
		}
	}
}